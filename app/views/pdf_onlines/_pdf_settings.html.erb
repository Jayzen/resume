<div class="col-md-10">
  <div class="card">
    <div class="card-header text-secondary text-center">
      <%= t('set_template') %>
    </div>
    <div class="col-md-8 m-auto"> 
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="https://test.zhengjiajun.com/uploads/wechat/name/28/middle_carousel_one.png" class="d-block w-100"/>
          <div class="carousel-caption d-none d-md-block">
            <h5 class="text-danger"><%= t('template_one') %></h5>
          </div>
        </div>
        <div class="carousel-item">
           <img src="https://test.zhengjiajun.com/uploads/wechat/name/27/middle_carousel_two.png" class="d-block w-100"/>
          <div class="carousel-caption d-none d-md-block">
            <h5 class="text-danger"><%= t('template_two') %></h5>
          </div>
        </div>
      </div>
      <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    </div>
    <div class="card-body">
      <%= link_to "模板1", demo_path, class: "btn btn-primary btn-sm" %>
      <%= link_to "模板2", demo_one_path, class: "btn btn-primary btn-sm" %>
    </div>  
  </div>
</div>
